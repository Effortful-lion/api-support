{
  "name": "api-support",
  "displayName": "Api Support",
  "description": "用于支持.api文件的语法高亮，用于自身学习使用",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.96.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "go",
      "aliases": ["Go", "go"],
      "extensions": [".api"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "go",
      "scopeName": "source.go",
      "path": "./syntaxes/go.tmLanguage.json"
    }]
  }
}
